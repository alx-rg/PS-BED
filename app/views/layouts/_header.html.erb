<header>
  <nav>
    <%= link_to image_tag("logo1.png"), root_path %>
    <ul>
      <li><%= link_to "Home", pets_path %></li>
    </ul>
    <div>
      <%= form_tag(pets_path, method: :get) do %>
      <%= text_field_tag :search, params[:search], placeholder: "Search pets" %>
      <%= submit_tag "Search" %>
      <% end %>
    </div>
  </nav>
</header>
